from _typeshed import Incomplete

class AlignmentIterator:
    handle: Incomplete
    records_per_alignment: Incomplete
    def __init__(self, handle, seq_count: Incomplete | None = None) -> None: ...
    def __next__(self) -> None: ...
    def __iter__(self): ...

class AlignmentWriter:
    handle: Incomplete
    def __init__(self, handle) -> None: ...
    def write_file(self, alignments) -> None: ...
    def clean(self, text): ...

class SequentialAlignmentWriter(AlignmentWriter):
    handle: Incomplete
    def __init__(self, handle) -> None: ...
    def write_file(self, alignments): ...
    def write_header(self) -> None: ...
    def write_footer(self) -> None: ...
    def write_alignment(self, alignment) -> None: ...
